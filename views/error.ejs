<% layout('layouts/boilerplate') %>
<div class="row">
   <div class="col-md-10 offset-md-1">
      <div class="alert alert-success" role="alert">
         <h4 class="alert-heading">Error <%= err.statusCode %></h4>
         <p><%= err.message %></p>
         <hr>
         <p class="mb-0"><%= err.stack %></p>
      </div>
   </div>
</div>